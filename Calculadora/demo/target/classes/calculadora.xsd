<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="http://www.example.org/calculadora" 
	xmlns:tns="http://www.example.org/calculadora" 
	elementFormDefault="qualified">
	

      <element name="SumaRequest">
        <complexType>
          <sequence>
            <element type="int" name="intA"/>
            <element type="int" name="intB"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      <element name="SumaResponse">
        <complexType>
          <sequence>
            <element  type="int" name="res"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      
      <element name="RestarRequest">
        <complexType>
          <sequence>
            <element type="int" name="intA"/>
            <element type="int" name="intB"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      <element name="RestarResponse">
        <complexType>
          <sequence>
            <element  type="int" name="res"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      
      <element name="MultiplicacionRequest">
        <complexType>
          <sequence>
            <element type="int" name="intA"/>
            <element type="int" name="intB"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      <element name="MultiplicacionResponse">
        <complexType>
          <sequence>
            <element  type="int" name="res"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      
      <element name="DividirRequest">
        <complexType>
          <sequence>
            <element type="int" name="intA"/>
            <element type="int" name="intB"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>
      <element name="DividirResponse">
        <complexType>
          <sequence>
            <element  type="int" name="res"/>
          </sequence>
          <attribute type="string" name="name"/>
        </complexType>
      </element>

</schema>